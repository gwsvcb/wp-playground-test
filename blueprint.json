{
  "meta": {
    "name": "GWS Playground Setup",
    "description": "A WordPress Playground configuration with ACF, site functionality plugins, and the bb-theme-child activated."
  },
  "phpExtensionBundles": ["core"],
  "plugins": [
    {
      "path": "/wp-content/plugins/gws-site-details",
      "activate": true
    },
    {
      "path": "/wp-content/plugins/gws-site-functionality",
      "activate": true
    }
  ],
  "themes": [
    {
      "path": "/wp-content/themes/bb-theme"
    },
    {
      "path": "/wp-content/themes/bb-theme-child",
      "activate": true
    },
    {
      "slug": "twentytwentyfive"
    }
  ]
}
